<Window x:Class="LABA_TOKAR_4_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LABA_TOKAR_4_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid d:DataContext="{d:DesignInstance Type=local:Customer}" x:Name="_grid">
        <Grid.RowDefinitions>
            <RowDefinition Height="24*"/>
            <RowDefinition Height="35*"/>
            <RowDefinition Height="360*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="{Binding FirstName}" Grid.Row="0" Margin="0,0,667,0"/>
        <TextBlock Text="{Binding LastName}"   Grid.Row="1" Margin="0,0,667,0"/>
        




    </Grid>


</Window>
